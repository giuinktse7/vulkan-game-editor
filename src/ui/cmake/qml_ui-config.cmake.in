@PACKAGE_INIT@

# Include the exported CMake file
get_filename_component(qml_ui_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)

# This macro enables usage of find_dependency().
# https://cmake.org/cmake/help/v3.11/module/CMakeFindDependencyMacro.html
include(CMakeFindDependencyMacro)

# Declare the used packages in order to communicate the requirements upstream.
find_dependency(common REQUIRED)

if(NOT TARGET qml_ui::qml_ui)
    include("${qml_ui_CMAKE_DIR}/qml_ui-targets.cmake")
endif()

check_required_components(qml_ui)